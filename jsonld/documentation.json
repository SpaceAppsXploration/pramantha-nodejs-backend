          "hydra:readonly": false,
          "hydra:writeonly": false
        },
        {
          "hydra:property": "skos:broader",
          "hydra:title": "Broader",
          "hydra:description": "Broader concepts.",
          "hydra:required": null,
          "hydra:readonly": false,
          "hydra:writeonly": false
        },
        {
          "hydra:property": "skos:prefLabel",
          "hydra:title": "Label",
          "hydra:description": "The preferred label for this concept.",
          "hydra:required": null,
          "hydra:readonly": false,
          "hydra:writeonly": false
        }
      ]
    }
  ]
